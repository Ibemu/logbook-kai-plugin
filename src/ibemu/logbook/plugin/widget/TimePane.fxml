<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.GridPane?>

<fx:root xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
         type="GridPane" styleClass="time-pane">
    <ProgressBar fx:id="progress" styleClass="progress" prefWidth="100.0" maxHeight="Infinity"/>
    <Label fx:id="time" styleClass="time"/>
</fx:root>
